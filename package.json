{
  "name": "paper-trading-system",
  "version": "1.0.0",
  "description": "Automated paper trading system with real-time scanning and strategy execution",
  "main": "server.ts",
  "scripts": {
    "start": "node dist/server.js",
    "start:simple": "ts-node server-simple.ts",
    "start:prod": "node server-simple.js",
    "start:production": "node server-simple.js",
    "dev": "ts-node server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start:all": "concurrently \"npm run start:gateway\" \"npm run start:scanner\" \"npm run start:database\" \"npm run start:notification\" \"npm run start:marketdata\" \"npm run start:auth\" \"npm run start:analysis\" \"npm run start:frontend\"",
    "start:gateway": "ts-node api/gateway.ts",
    "start:scanner": "ts-node api/microservices/ScannerService.ts",
    "start:database": "ts-node api/microservices/DatabaseService.ts",
    "start:notification": "ts-node api/microservices/NotificationService.ts",
    "start:marketdata": "ts-node api/microservices/MarketDataService.ts",
    "start:auth": "ts-node api/microservices/AuthService.ts",
    "start:analysis": "ts-node api/microservices/EndOfDayService.ts",
    "start:frontend": "cd frontend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "gateway": "ts-node api/gateway.ts",
    "scanner": "ts-node api/microservices/ScannerService.ts",
    "database": "ts-node api/microservices/DatabaseService.ts",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@types/node": "^20.0.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.0",
    "openai": "^5.9.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "winston": "^3.11.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^8.10.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "trading",
    "algorithmic",
    "paper-trading",
    "fintech",
    "typescript",
    "nodejs",
    "backtesting",
    "optimization",
    "analysis"
  ],
  "author": "Paper Trading System",
  "license": "MIT"
}
